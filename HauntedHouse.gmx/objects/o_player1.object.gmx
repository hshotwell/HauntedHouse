if (place_meeting(x+hspd, y, o_solid))
{
    while (!place_meeting(x+sign(hspd), y, o_solid))
    {
        x+= sign(hspd);
    }
    hspd = 0;
}

// Move horizontally
x += hspd;

// Vertical collisions
if (place_meeting(x, y+vspd, o_solid))
{
    while (!place_meeting(x, y+sign(vspd), o_solid))
    {
        y+= sign(vspd);
    }
    vspd = 0;
}

// Move vertically
y += vspd;

draw_xscale = lerp(draw_xscale, 1, .1);
draw_yscale = lerp(draw_yscale, 1, .1);;

if (place_meeting(x, y+1, o_solid) &amp;&amp; !place_meeting(x, yprevious+1, o_solid))
{
    draw_xscale = .75;
    draw_yscale = 1.25;
}

scr_cam_move();
